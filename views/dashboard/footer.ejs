<footer>
  <link rel="stylesheet" href="css/footer.css">

  <section class='new-contribution'>
    <h4>Contribute to the Project</h4>
    <form class="contribution-form flex-container flex-column" action="" method=''>
      <div class="text-field-container">
        <label class='text-field-header' for="aid-submit-title">What are you contributing?</label>
        <input class='text-field input-field' id='aid-submit-title' type="text" name="title" placeholder='Bok Choy, Steel Cut Oats, etc...' value="">
      </div>
      <div class="food-type-selection">
        <h5>Food Type</h5>
        <div class="flex-container type-selector">

          <label class='input-field' for="">
            <input type="radio" name="foodType" value="fruit">
            Fruit
          </label>


          <label class='input-field' for="">
            <input type="radio" name="foodType" value="vegetable">
            Vegetable
          </label>


          <label class='input-field' for="">
            <input type="radio" name="foodType" value="grain">
            Grain
          </label>
        </div>
      </div>
      <div class="aid-origin-container">
        <div class="aid-origin">
          <label for="is-garden-grown">
            <span>Garden Grown</span>
          </label>
        </div>
        <input class='toggle-switch-checkbox' id='is-garden-grown' type="checkbox" name="is-garden-grown" value="">
        <label class='toggle-switch-label' for="is-garden-grown"></label>
      </div>
      <div class="expiration-container">
        <label for="expiration-date-input">Expiration</label>
        <input class='input-field' id='expiration-date-input' type="date" name="expiration" value="">
      </div>








      <button id='submit-new-aid' type="button" name="button">Submit</button>

    </form>
    <div class="contribute-button-container">
      <button class='contribute-button' type="button" name="button">
        <img id='addAidButton' src="img/icons/plus-red.png" alt="Contribute">
      </button>
    </div>

  </section>
  <section class='flex-container nav-container'>
    <nav class=''>
      <ul class='flex-container'>
        <li>
          <img class='icon' src="img/icons/house-icon.png" alt="Home">
        </li>
        <li>
          <img class='icon' src="img/icons/food-icon.png" alt="Food Aid">
        </li>
        <li>
          <img class='icon' src="img/icons/governance-icon.png" alt="Governance and Contribution">
        </li>
      </ul>
    </nav>
    <div class="under-curve">
    </div>
    <div class="right-curve">
    </div>
  </section>
<script type="text/javascript" src='js/footer.js'></script>
</footer>
